<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">  
   <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <display-name>MyProject2</display-name>
  <servlet>
    <servlet-name>useradd</servlet-name>
    <servlet-class>domain.UserAdd</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>useradd</servlet-name>
    <url-pattern>/add</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>usersite</servlet-name>
    <servlet-class>domain.UserSite</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>usersite</servlet-name>
    <url-pattern>/site</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>login</servlet-name>
    <servlet-class>domain.Login</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>premium</servlet-name>
    <servlet-class>domain.UserPremium</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>premium</servlet-name>
  <url-pattern>/premiumSet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>premiumDel</servlet-name>
    <servlet-class>domain.UserPremiumDel</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>premiumDel</servlet-name>
    <url-pattern>/premiumDel</url-pattern>
  </servlet-mapping>

     <servlet>
    <servlet-name>showall</servlet-name>
    <servlet-class>domain.ShowAllUsers</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>showall</servlet-name>
    <url-pattern>/showall</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>logout</servlet-name>
    <servlet-class>domain.Logout</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>logout</servlet-name>
    <url-pattern>/logout</url-pattern>
  </servlet-mapping>


</web-app>

